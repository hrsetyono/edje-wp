  "scripts": {
    "sass-editor": "node-sass -w module-editor/sass -o module-editor/css --output-style compressed",
    "bsync-editor": "browser-sync start --proxy \"lab.test/\" --files \"module-editor/css/*.css\"",

    "sass-vendor": "node-sass -w module-vendor/sass -o module-vendor/css --output-style compressed",

    "sass-faq": "node-sass -w module-editor-faq/sass -o module-editor-faq/css --output-style compressed",
    "bsync-faq": "browser-sync start --proxy \"lab.test/\" --files \"module-editor-faq/assets/*.css\"",

    "sass": "node-sass -w ./ -o ./ --output-style compressed"
  }
