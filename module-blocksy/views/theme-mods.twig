<!-- Theme Mods -->
<style type="text/css">
{{ _self.css_output( desktop_css ) }}
</style>

<style type="text/css" media="(max-width: 767px)">
{{ _self.css_output( tablet_css ) }}
</style>

<style type="text/css" media="(max-width: 480px)">
{{ _self.css_output( mobile_css ) }}
</style>


{% macro css_output( css ) %}
{% for selector, styles in css %}
{{ selector }} {
  {% for prop, value in styles %}
  {{ prop }}: {{ value }};
  {% endfor %}
}
{% endfor %}
{% endmacro %}